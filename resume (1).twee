:: StoryScript [script]
// –§—É–Ω–∫—Ü–∏—è –¥–ª—è –Ω–∞–≤–∏–≥–∞—Ü–∏–∏ –º–µ–∂–¥—É –ø–∞—Å—Å–∞–∂–∞–º–∏
function goToPassage(passageName) {
    const passages = document.querySelectorAll('tw-passage');
    passages.forEach(passage => {
        passage.style.display = 'none';
    });
    
    const targetPassage = Array.from(passages).find(p => 
        p.getAttribute('data-name') === passageName
    );
    
    if (targetPassage) {
        targetPassage.style.display = 'block';
        // –ü–ª–∞–≤–Ω–∞—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∞ –∫ –Ω–∞—á–∞–ª—É
        window.scrollTo({
            top: 0,
            behavior: 'smooth'
        });
    }
}

// –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è –∫–∞—Ä—Ç–∏–Ω–∫–∏ –≤ –ø–æ–ª–Ω–æ–º —Ä–∞–∑–º–µ—Ä–µ
function openModal(element) {
    const modal = document.getElementById("imageModal");
    const modalImg = document.getElementById("modalImage");
    const img = element.querySelector('img');
    
    modal.style.display = "flex";
    modalImg.src = img.src;
    modalImg.alt = img.alt;
    
    // –ë–ª–æ–∫–∏—Ä—É–µ–º –ø—Ä–æ–∫—Ä—É—Ç–∫—É —Ñ–æ–Ω–∞
    document.body.style.overflow = 'hidden';
}

function closeModal() {
    const modal = document.getElementById("imageModal");
    modal.style.display = "none";
    // –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –ø—Ä–æ–∫—Ä—É—Ç–∫—É
    document.body.style.overflow = 'auto';
}

// –ó–∞–∫—Ä—ã—Ç–∏–µ –º–æ–¥–∞–ª—å–Ω–æ–≥–æ –æ–∫–Ω–∞ –ø—Ä–∏ –∫–ª–∏–∫–µ –≤–Ω–µ –∫–∞—Ä—Ç–∏–Ω–∫–∏
document.addEventListener('DOMContentLoaded', function() {
    const modal = document.getElementById("imageModal");
    
    if (modal) {
        modal.onclick = function(e) {
            if (e.target.className === "modal") {
                closeModal();
            }
        };
    }
    
    // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –ø–µ—Ä–≤—ã–π –ø–∞—Å—Å–∞–∂ –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ
    const passages = document.querySelectorAll('tw-passage');
    passages.forEach((passage, index) => {
        if (index === 0) {
            passage.style.display = 'block';
        } else {
            passage.style.display = 'none';
        }
    });
    
    // –î–æ–±–∞–≤–ª—è–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –¥–ª—è –≤—Å–µ—Ö –∫–Ω–æ–ø–æ–∫ –Ω–∞–≤–∏–≥–∞—Ü–∏–∏
    document.querySelectorAll('a[onclick^="goToPassage"]').forEach(link => {
        link.onclick = function(e) {
            e.preventDefault();
            const match = this.getAttribute('onclick').match(/goToPassage\('([^']+)'\)/);
            if (match) {
                goToPassage(match[1]);
            }
        };
    });
});

// –ê–Ω–∏–º–∞—Ü–∏—è –ø–æ—è–≤–ª–µ–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –ø—Ä–∏ –ø—Ä–æ–∫—Ä—É—Ç–∫–µ
function animateOnScroll() {
    const elements = document.querySelectorAll('.task, .skill-card, .gallery-item');
    
    elements.forEach(element => {
        const elementTop = element.getBoundingClientRect().top;
        const windowHeight = window.innerHeight;
        
        if (elementTop < windowHeight - 50) {
            element.style.opacity = '1';
            element.style.transform = 'translateY(0)';
        }
    });
}

// –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –∞–Ω–∏–º–∞—Ü–∏–π
document.addEventListener('DOMContentLoaded', function() {
    // –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –Ω–∞—á–∞–ª—å–Ω—ã–µ —Å—Ç–∏–ª–∏ –¥–ª—è –∞–Ω–∏–º–∞—Ü–∏–∏
    const animatedElements = document.querySelectorAll('.task, .skill-card, .gallery-item');
    animatedElements.forEach(el => {
        el.style.opacity = '0';
        el.style.transform = 'translateY(20px)';
        el.style.transition = 'opacity 0.6s ease, transform 0.6s ease';
    });
    
    // –ó–∞–ø—É—Å–∫–∞–µ–º –∞–Ω–∏–º–∞—Ü–∏—é –ø–æ—Å–ª–µ –Ω–µ–±–æ–ª—å—à–æ–π –∑–∞–¥–µ—Ä–∂–∫–∏
    setTimeout(() => {
        animateOnScroll();
    }, 300);
    
    // –î–æ–±–∞–≤–ª—è–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫ –ø—Ä–æ–∫—Ä—É—Ç–∫–∏
    window.addEventListener('scroll', animateOnScroll);
});

// –ü—Ä–æ—Å—Ç–∞—è —Å–∏—Å—Ç–µ–º–∞ –ø—Ä–æ–≥—Ä–µ—Å—Å–∞
function updateProgress(passageName) {
    const progress = document.getElementById('progress-bar');
    if (!progress) return;
    
    const passages = ['–ù–∞—á–∞–ª–æ', '–ó–∞–¥–∞—á–∏', '–í—ã–≤–æ–¥—ã'];
    const currentIndex = passages.indexOf(passageName);
    
    if (currentIndex !== -1) {
        const percentage = ((currentIndex + 1) / passages.length) * 100;
        progress.style.width = percentage + '%';
        progress.textContent = Math.round(percentage) + '%';
    }
}

// –í—ã–∑—ã–≤–∞–µ–º –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –ø—Ä–æ–≥—Ä–µ—Å—Å–∞ –ø—Ä–∏ —Å–º–µ–Ω–µ –ø–∞—Å—Å–∞–∂–∞
function goToPassageWithProgress(passageName) {
    goToPassage(passageName);
    updateProgress(passageName);
}